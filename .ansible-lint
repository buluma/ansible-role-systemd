---
#
# Ansible managed
#
exclude_paths:
  - ./meta/preferences.yml
  - ./molecule/default/prepare.yml
  - ./molecule/default/converge.yml
  - ./molecule/default/verify.yml
  - ./molecule/default/collections.yml
  - ./.tox
  - ./.cache
  - ./.github
  - ./requirements.yml

skip_list:
  - yaml[truthy]
  - ignore-errors
  - no-handler
  - key-order[task]
  - no-changed-when
  - name[prefix]

enable_list:
  - name[prefix]
